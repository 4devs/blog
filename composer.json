{
    "name": "fdevs/blog",
    "license": "MIT",
    "type": "project",
    "description": "Blog based on the Symfony 2",
    "homepage": "http://4devs.io",
    "autoload": {
        "psr-0": { "": "src/", "SymfonyStandard": "app/" }
    },
    "require": {
        "php": ">=5.4",
        "ext-mongo": ">=1.2.12,<1.7-dev",
        "symfony/symfony": "~2.6",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.3",
        "symfony/icu": "1.1.*",
        "sensio/distribution-bundle": "~3.0",
        "sensio/framework-extra-bundle": "~3.0",
        "jms/security-extra-bundle": "1.5.*",
        "jms/di-extra-bundle": "1.4.*",
        "doctrine/mongodb-odm": "1.0.*@dev",
        "doctrine/mongodb-odm-bundle": "3.0.*@dev",
        "doctrine/doctrine-cache-bundle": "~1.0",
        "doctrine/doctrine-fixtures-bundle": "~2.2",
        "friendsofsymfony/user-bundle": "2.0.*@dev",
        "helios-ag/fm-elfinder-bundle": "~1.4",
        "sonata-project/doctrine-mongodb-admin-bundle": "2.2.x-dev",
        "sonata-project/block-bundle": "~2.2",
        "sonata-project/core-bundle": "~2.2",
        "sonata-project/admin-bundle": "~2.2",
        "dizda/cloud-backup-bundle": "~1.5.3",
        "knplabs/knp-menu-bundle": "~1.1",
        "knplabs/knp-disqus-bundle": "~1.0",
        "knplabs/knp-paginator-bundle": "~2.3",
        "knplabs/knp-menu": "~1.1",
        "incenteev/composer-parameter-handler": "~2.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "symfony-assets-install": "symlink"
    }
}
