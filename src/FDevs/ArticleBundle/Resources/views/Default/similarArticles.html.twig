{% if articles | length %}
    <h5>{{ 'Читайте также' | trans }}</h5>
    <ul>
        {% for article in articles %}
            <li>
                <a href="{{ path('f_devs_article_article', {slug: article.id })}}">{{ article.title }}</a>
            </li>
        {% endfor %}
    </ul>
{% endif %}
